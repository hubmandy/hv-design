        // Gestion des sections
        function showSection(section) {
            document.querySelectorAll('.section-content').forEach(s => s.classList.remove('active'));
            document.querySelectorAll('.section-btn').forEach(b => b.classList.remove('active'));
            
            document.getElementById('section-' + section).classList.add('active');
            event.target.classList.add('active');
        }

        // S√©lection radio avec style
        function selectRadio(element) {
            const group = element.closest('.radio-group, .wood-selector');
            group.querySelectorAll('.radio-option, .wood-option').forEach(opt => {
                opt.classList.remove('selected');
            });
            element.classList.add('selected');
            element.querySelector('input').checked = true;
        }

        // S√©lection couleur
        function selectColor(element) {
            document.querySelectorAll('.color-option').forEach(opt => {
                opt.classList.remove('selected');
            });
            element.classList.add('selected');
            element.querySelector('input').checked = true;
        }

        // S√©lection bois
        function selectWood(element) {
            document.querySelectorAll('.wood-option').forEach(opt => {
                opt.classList.remove('selected');
            });
            element.classList.add('selected');
            element.querySelector('input').checked = true;
        }

        // Mise √† jour nom fichier
        function updateFileName(input, targetId) {
            const target = document.getElementById(targetId);
            if (input.files.length > 0) {
                if (input.files.length === 1) {
                    target.textContent = '‚úÖ ' + input.files[0].name;
                } else {
                    target.textContent = '‚úÖ ' + input.files.length + ' fichiers s√©lectionn√©s';
                }
            }
        }

        // Fonction d'affichage des messages
        function showMessage(type, message) {
            const successDiv = document.getElementById('successMessage');
            const errorDiv = document.getElementById('errorMessage');
            const loading = document.getElementById('loading');
            
            loading.style.display = 'none';
            
            if (type === 'success') {
                successDiv.textContent = message;
                successDiv.style.display = 'block';
                errorDiv.style.display = 'none';
                
                setTimeout(() => {
                    successDiv.style.display = 'none';
                }, 5000);
            } else {
                errorDiv.textContent = message;
                errorDiv.style.display = 'block';
                successDiv.style.display = 'none';
            }
        }

        // Soumission formulaire PLATEAUX
        document.getElementById('formPlateaux').addEventListener('submit', async function(e) {
            e.preventDefault();
            
            const loading = document.getElementById('loading');
            loading.style.display = 'block';
            
            const formData = new FormData(this);
            formData.append('type_demande', 'Plateau');
            
            formData.set('option_gravure', formData.get('option_gravure') ? 'Oui' : 'Non');
            
            try {
                const response = await fetch(SCRIPT_URL, {
                    method: 'POST',
                    body: formData
                });
                
                const result = await response.json();
                
                if (result.status === 'success') {
                    showMessage('success', '‚úÖ Votre demande a √©t√© envoy√©e avec succ√®s ! Nous vous r√©pondrons rapidement par email.');
                    this.reset();
                    document.querySelectorAll('.selected').forEach(el => el.classList.remove('selected'));
                } else {
                    throw new Error(result.message || 'Erreur lors de l\'envoi');
                }
            } catch (error) {
                showMessage('error', '‚ùå Erreur : ' + error.message + '. Veuillez r√©essayer ou nous contacter directement.');
            }
        });

        // Soumission formulaire PRESTATIONS
        document.getElementById('formPrestations').addEventListener('submit', async function(e) {
            e.preventDefault();
            
            const loading = document.getElementById('loading');
            loading.style.display = 'block';
            
            const formData = new FormData(this);
            formData.append('type_demande', 'Prestation');
            
            try {
                const response = await fetch(SCRIPT_URL, {
                    method: 'POST',
                    body: formData
                });
                
                const result = await response.json();
                
                if (result.status === 'success') {
                    showMessage('success', '‚úÖ Votre demande a √©t√© envoy√©e avec succ√®s ! Nous vous r√©pondrons rapidement par email.');
                    this.reset();
                    document.querySelectorAll('.selected').forEach(el => el.classList.remove('selected'));
                } else {
                    throw new Error(result.message || 'Erreur lors de l\'envoi');
                }
            } catch (error) {
                showMessage('error', '‚ùå Erreur : ' + error.message + '. Veuillez r√©essayer ou nous contacter directement.');
            }
        });
    </script>
</body>
</html>
